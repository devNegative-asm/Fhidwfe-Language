function void cls() {
	putchar$ 27ub
	puts$ "[2J"
}

function void random_scramble(cube:Cube) {
	puts$ "spam the keyboard (seed): "
	temp line = stdin.next_line$
	temp cstr = line.c_str$
	seed = @uint string_hash$ @listubyte cstr
	cls$
	cube.print$
	nanosleep$ 800000000u
	for int [1,21] with i {
		nanosleep$ 300000000u
		cls$
		cube.print$
		bits = & 7u seed
		seed = >> >> >> seed
		if = bits 0u {
			cube.R$
		} ;
		if = bits 1u {
			cube.L$
		} ;
		if = bits 2u {
			cube.U$
		} ;
		if = bits 3u {
			cube.D$
		} ;
		if = bits 4u {
			cube.B$
		} ;
		if = bits 5u {
			cube.F$
		} ;
		if = bits 6u {
			cube.S$
		} ;
		if = bits 7u {
			cube.M$
		} ;
	}
	
}

