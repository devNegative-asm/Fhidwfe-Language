function void pw2(char:ubyte) {
	putchar$ 27ub
	putchar$ '['


	if = char 'Y' {
		puts$ "103m"
	} ;
	if = char 'G' {
		puts$ "102m"
	} ;
	if = char 'R' {
		puts$ "41m"
	} ;
	if = char 'B' {
		puts$ "104m"
	} ;
	if = char 'W' {
		puts$ "47m"
	} ;
	if = char 'O' {
		puts$ "101m"
	} ;
	puts$ "  "
	putchar$ 27ub
	puts$ "[0m"
}

